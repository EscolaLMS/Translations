{
  "name": "escolalms/translations",
  "description": "Escola Headless LMS Translations",
  "type": "package",
  "require": {
    "php": ">=7.4 | >=8.0",
    "escolalms/auth": "^0",
    "escolalms/core": "^1",
    "laravel/framework": ">=8.0",
    "spatie/laravel-translation-loader": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": ">=5.0"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Maciej Rymarz",
      "email": "maciej.rymarz@escolasoft.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EscolaLms\\Translations\\": "src",
      "EscolaLms\\Translations\\Tests\\": "tests",
      "EscolaLms\\Translations\\Database\\Seeders\\": "database/seeders",
      "EscolaLms\\Translations\\Database\\Factories\\": "database/factories"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "EscolaLms\\Translations\\EscolaLmsTranslationsServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
